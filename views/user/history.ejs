<h4>Unique sign in IP-addresses <small class="text-muted">(<%= _.size(ipData) %>)</small></h4>

<table id="userSignInsUniqueIps" class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th class="col-md-1 text-nowrap">IP-Address</th>
            <th class="col-md-1 text-nowrap text-right">Sign in count</th>
            <th class="col-md-1 text-nowrap">Last sign in time</th>
            <th class="col-md-9">Last sign in user-agent</th>
        </tr>
        <tbody>
        <% _.each(ipData, function(row, ip) { %>
        <tr>
            <td class="text-nowrap">
                <small><%= ip %></small>
            </td>
            <td class="text-nowrap text-right">
                <small><%= row.length %></small>
            </td>
            <td class="text-nowrap">
                <small class="tooltipDiv">
                    <%= row[0].stamp.format(currentUser.momentFormatDateTime) %>

                    <%- partial("../partials/tooltip/datetime", {tooltipTitle: false, stamp: row[0].stamp}) %>
                </small>
            </td>
            <td class="text-nowrap">
                <small><%= row[0].agent %></small>
            </td>
        </tr>
        <% }); %>
    </thead>
</table>

<h4>Unique sign in user-agents <small class="text-muted">(<%= _.size(agentData) %>)</small></h4>

<table id="userSignInsUniqueIps" class="table table-striped table-hover table-condensed">
    <thead>
    <tr>
        <th class="col-md-9">User-agent</th>
        <th class="col-md-1 text-nowrap text-right">Sign in count</th>
        <th class="col-md-1 text-nowrap">Last sign in time</th>
        <th class="col-md-1 text-nowrap">Last sign in IP-Address</th>
    </tr>
    <tbody>
    <% _.each(agentData, function(row, agent) { %>
    <tr>
        <td class="text-nowrap">
            <small><%= agent %></small>
        </td>
        <td class="text-nowrap text-right">
            <small><%= row.length %></small>
        </td>
        <td class="text-nowrap">
            <small class="tooltipDiv">
                <%= row[0].stamp.format(currentUser.momentFormatDateTime) %>

                <%- partial("../partials/tooltip/datetime", {tooltipTitle: false, stamp: row[0].stamp}) %>
            </small>
        </td>
        <td class="text-nowrap">
            <small><%= row[0].ip %></small>
        </td>
    </tr>
    <% }); %>
    </thead>
</table>


<h4>Full sign in list <small class="text-muted">(<%= _.size(history) %>)</small></h4>

<table id="userSignInsFullList" class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th class="col-md-1 text-nowrap">Sign in time</th>
            <th class="col-md-1 text-nowrap">IP-Address</th>
            <th class="col-md-10">User-agent</th>
        </tr>
    </thead>
    <tbody>
    <% _.each(history, function(row) { %>
        <tr>
            <td class="text-nowrap">
                <small class="tooltipDiv">
                    <%= row.stamp.format(currentUser.momentFormatDateTime) %>

                    <%- partial("../partials/tooltip/datetime", {tooltipTitle: false, stamp: row.stamp}) %>
                </small>
            </td>
            <td class="text-nowrap">
                <small><%= row.ip %></small>
            </td>
            <td>
                <small><%= row.agent %></small>
            </td>
        </tr>
    <% }); %>
    </tbody>
</table>