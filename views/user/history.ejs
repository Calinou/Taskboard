<h4>Unique sign in IP-addresses <small class="text-muted">(<%= _.size(ipData) %>)</small></h4>

<table id="userSignInsUniqueIps" class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th class="col-md-1 text-nowrap">IP-Address</th>
            <th class="col-md-1 text-nowrap text-right">Sign in count</th>
            <th class="col-md-1 text-nowrap">Last sign in time</th>
            <th class="col-md-9">Last sign in user-agent</th>
        </tr>
        <tbody>
        <% _.each(ipData, function(row, ip) { %>
        <tr>
            <td class="text-nowrap">
                <small><%= ip %></small>
            </td>
            <td class="text-nowrap text-right">
                <small><%= row.length %></small>
            </td>
            <td class="text-nowrap">
                <small class="tooltipDiv">
                    <%= row[0].stamp.format(currentUser.momentFormatDateTime) %>

                    <div class="tooltipDivContainer">
                        <div>
                            <table class="dateTimeTooltip">
                                <tr>
                                    <th>Local time</th>
                                    <td><%= row[0].stamp.format(currentUser.momentFormatDateTime) %></td>
                                </tr>
                                <tr>
                                    <th></th>
                                    <td class="text-muted">
                                        <%= row[0].stamp.tz() %> (<%= row[0].stamp.format("ZZ") %>)
                                    </td>
                                </tr>
                                <tr>
                                    <th>UTC time</th>
                                    <td><%= row[0].stamp.utc().format(currentUser.momentFormatDateTime) %></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </small>
            </td>
            <td class="text-nowrap">
                <small><%= row[0].agent %></small>
            </td>
        </tr>
        <% }); %>
    </thead>
</table>

